<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Title>Rabbit Hole</Title>
    <Product>Rabbit Hole</Product>
    <Version>0.2.8</Version>
    <Company>ChatAIze</Company>
    <Authors>Marcel Kwiatkowski</Authors>
    <Copyright>Â© ChatAIze 2025</Copyright>
    <AssemblyName>ChatAIze.RabbitHole</AssemblyName>
    <RootNamespace>ChatAIze.RabbitHole</RootNamespace>
    <PackageId>ChatAIze.RabbitHole</PackageId>
    <PackageIcon>icon.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageProjectUrl>https://www.chataize.com</PackageProjectUrl>
    <PackageReleaseNotes>https://github.com/chataize/rabbit-hole/releases</PackageReleaseNotes>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/chataize/rabbit-hole</RepositoryUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <SuppressSymbolPackageFormatValidation>true</SuppressSymbolPackageFormatValidation>
    <Description>
      C# library for scraping text content from websites.
    </Description>
    <PackageTags>
      .net, c#, content, csharp, document, documents, download, downloader, downloading, extract,
      extracting, extractor, html, http, https, library, link, links, net, online, page, pages,
      parse, parser, parsing, scrape, scraper, scraping, search, searching, site, sites, string,
      strings, text, uri, uris, url, urls, web, webpage, webpages, website, websites
    </PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <None Include="icon.png" Pack="true" PackagePath="\" />
    <None Include="../README.md" Pack="true" PackagePath="" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack" Version="1.12.1" />
  </ItemGroup>
</Project>
